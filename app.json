{
  "expo": {
    "name": "tictac",
    "slug": "tic-tac",
    "version": "2.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/tictacicon.png",
    "scheme": "tictac",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#0f2027",
        "foregroundImage": "./assets/images/tictacicon.png",
        "backgroundImage": "./assets/images/tictacicon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.kigunda.tictac"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/tictacicon.png"
    },
    "splash": {
      "text": "",
      "image": "./assets/images/output-onlinepngtools.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f2027"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/output-onlinepngtools.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#0f2027",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "086313a2-44ff-454e-985c-c32144513c95"
      }
    }
  }
}
